# Environment Variables Example
# Copy this file to .env and fill in your actual values

# Boson AI API Configuration
BOSON_API_KEY=your_api_key_here
BOSON_API_URL=https://api.boson.ai

# Audio Configuration
AUDIO_SAMPLE_RATE=16000
AUDIO_CHANNELS=1

# Car Hardware Configuration
CAR_SERIAL_PORT=/dev/ttyUSB0
CAR_BAUD_RATE=115200

# Debug Settings
DEBUG_MODE=false
LOG_LEVEL=INFO
